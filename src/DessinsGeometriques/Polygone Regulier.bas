10 'POLYGONES REGULIERS
50 LPRINT CHR$C18) :LPRINT'"M0,-500" :LPRINT"I":NP=480:PI=ATN(1)*4
100 K=4:CX=NP/2:CY=NP/2:R=NP*.45:AD=PI/4
200 FOR I=0 TO K
210 X=CX+R*COS(2*I*PI/K+AD):X%=INT(X)
220 Y=CY+R*SIN(2*I*PI/K+AD):Y%=INTCY)
250 IP I=0 THEN LPRINT"M";X%;",";Y%
260 IF I>0 THEN LPRINT"D";X%;",";Y%
300 NEXT I
500 END