10 CLS:CLEAR 500:INPUT"Quel code ?";M
20 FOR I=1 TO 8
30 INPUT"NOMBRE ";X$(I):NEXT I
40 FOR I=1 TO 8
50 FOR H=1 TO 6
60 Q=VAL(MID$(X$(I),H,1))
70 X(I)=X(I)+Q*2^(8-H):NEXT H,I
80 D$=""
90 FOR I=1 TO 8
100 D$=D$+STR$(X(I))+",":NEXT I
110 B=LEN(D$):D$=LEFT$(D$,B-1)
120 FONT$(M)=D$:PRINT CHR$(M)